<server description="Sample Servlet server">
    <featureManager>
        <feature>jaxrs-2.1</feature>
        <feature>jsonp-1.1</feature>
    </featureManager>

    <httpEndpoint httpPort="${default.http.port}" httpsPort="${default.https.port}" id="defaultHttpEndpoint"  host="*">
        <httpOptions removeServerHeader="false"/>
    </httpEndpoint>

    <webApplication id="OpenLibertyBenchmark" location="OpenLibertyBenchmark.war" contextRoot="${app.context.root}" />
</server>
